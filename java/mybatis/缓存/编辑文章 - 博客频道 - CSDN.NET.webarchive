    var codePlugin = {
    	Code: {
    		c: 'Code', t: 'æ’å…¥ä»£ç ', h: 1, e: function () {
    			var _this = this;
    			var jTest = $(htmlCode);
    			var jSave = $('#xheSave', jTest);
